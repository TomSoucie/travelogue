<h1>Welcome to Travelogue</h1>
<h3>Login or Sign Up below</h3>

<%= form_tag '/login' do %>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>

<%= link_to "Not registered? Sign Up Here!", new_user_path %>
